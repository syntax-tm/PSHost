<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <SolidColorBrush x:Key="CaretColor" Color="#CDFF95" />
    <SolidColorBrush x:Key="SelectionColor" Color="#95FFFF" />

    <SolidColorBrush x:Key="DarkBackgroundColor" Color="#1D1D1D" />
    <SolidColorBrush x:Key="DarkestBackgroundColor" Color="#121212" />
    <SolidColorBrush x:Key="LightBackgroundColor" Color="#F1F1F1" />
    <SolidColorBrush x:Key="LightestBackgroundColor" Color="#F9F9F9" />

    <SolidColorBrush x:Key="DarkForegroundColor" Color="#1D1D1D" />
    <SolidColorBrush x:Key="DarkestForegroundColor" Color="#121212" />
    <SolidColorBrush x:Key="LightForegroundColor" Color="#F1F1F1" />
    <SolidColorBrush x:Key="LightestForegroundColor" Color="#F9F9F9" />

    <SolidColorBrush x:Key="BorderColor" Color="#7F7F7F" />

    <SolidColorBrush x:Key="DisabledForegroundColor" Color="#9D9D9D" />

    <SolidColorBrush x:Key="CommandOutputColor" Color="#A5F9FF" />
